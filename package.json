{
  "name": "your-company-api",
  "version": "1.0.0",
  "description": "Express RESTful APIs for your-company",
  "author": "Sudhanshu sharma <sudhanshusharma647@gmail.com>",
  "main": "src/index.js",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=8",
    "yarn": "*"
  },
  "scripts": {
    "db:migrate": "knex migrate:latest",
    "db:unmigrate": "knex migrate:rollback",
    "db:seed": "knex seed:run",
    "precommit": "yarn lint",
    "predev": "yarn db:migrate",
    "prestart": "yarn db:migrate",
    "start": "cross-env NODE_ENV=production node ./src/index.js",
    "dev": "nodemon ./src/index.js",
    "lint": "eslint ./src/ --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:fix": "yarn lint --fix",
    "lint:watch": "yarn lint --watch",
    "test": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha --timeout 20000 --exit --recursive src/api/tests",
    "test:unit": "cross-env NODE_ENV=test mocha src/api/tests/unit",
    "test:integration": "cross-env NODE_ENV=test mocha --timeout 20000 --exit src/api/tests/integration",
    "test:watch": "cross-env NODE_ENV=test mocha --watch src/api/tests/unit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "postcoverage": "open coverage/lcov-report/index.html",
    "validate": "yarn lint && yarn test",
    "postpublish": "git push --tags"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "passport",
    "es6",
    "es7",
    "es8",
    "es2017",
    "mocha",
    "istanbul",
    "nyc",
    "eslint",
    "Travis CI",
    "coveralls",
    "REST",
    "API",
    "generator"
  ],
  "dependencies": {
    "aws-sdk": "^2.688.0",
    "axios": "^0.19.2",
    "bcryptjs": "2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv-safe": "^8.2.0",
    "email-templates": "^7.0.5",
    "express": "^4.17.1",
    "express-validation": "^3.0.5",
    "helmet": "^3.22.0",
    "http-status": "^1.4.2",
    "jwt-simple": "0.5.6",
    "knex": "^0.21.1",
    "lodash": "^4.17.15",
    "method-override": "^3.0.0",
    "moment-timezone": "^0.5.31",
    "morgan": "^1.10.0",
    "mysql2": "^2.1.0",
    "nodemailer": "^6.4.6",
    "nodemailer-ses-transport": "^1.5.1",
    "objection": "^2.1.5",
    "passport": "^0.4.1",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "4.0.0",
    "pm2": "^4.4.0",
    "pug": "^3.0.0",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4",
    "uuid": "^8.1.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.1.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "husky": "^4.2.5",
    "mocha": "^7.2.0",
    "nodemon": "^2.0.4",
    "nyc": "^15.0.1",
    "open-cli": "^6.0.1",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "supertest": "^4.0.2"
  }
}